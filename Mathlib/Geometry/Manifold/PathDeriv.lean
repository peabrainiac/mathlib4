def unitInterval.val' (t : unitInterval) : ‚Ñù¬π := EuclideanSpace.single 0 t

def unitInterval.proj' (x : ‚Ñù¬π) : unitInterval := Set.projIcc 0 1 zero_le_one (x 0)

