lemma unitInterval.mfderivWithin_from_real {f : unitInterval ‚Üí unitInterval} {s : Set unitInterval}
    {t : unitInterval} (hf : MDifferentiableWithinAt (ùì°‚àÇ 1) (ùì°‚àÇ 1) f s t)
    (hst : UniqueMDiffWithinAt (ùì°‚àÇ 1) s t) :
    mfderivWithin (ùì°‚àÇ 1) (ùì°‚àÇ 1) f s t = if t.val < 1 ‚àß (f t).val < 1 ‚à® t.val = 1 ‚àß (f t).val = 1
      then fderivWithin ‚Ñù (val' ‚àò f ‚àò proj') (val' '' s) (val' t)
      else -fderivWithin ‚Ñù (val' ‚àò f ‚àò proj') (val' '' s) (val' t) := by
  have hst' : UniqueMDiffWithinAt (ùì° 1) (val' '' s) (val' t) := by
    refine' hst.image_denseRange smooth_val'.mdifferentiableWithinAt.hasMFDerivWithinAt _
    rw [mfderivWithin_subset s.subset_univ hst smooth_val'.mdifferentiableWithinAt,
      mfderivWithin_univ,mfderiv_val']
    by_cases ht : t.val < 1
    all_goals simp only [ht,ite_true,ite_false]
    exact denseRange_id
    have h : LinearMap.range (-ContinuousLinearMap.id ‚Ñù ‚Ñù¬π) =
      LinearMap.range (ContinuousLinearMap.id ‚Ñù ‚Ñù¬π) := LinearMap.range_neg _
    rw [DenseRange,‚ÜêLinearMap.range_coe,h,LinearMap.range_coe]
    exact denseRange_id
  rw [‚ÜêmfderivWithin_eq_fderivWithin,
    mfderivWithin_comp _ smooth_val'.mdifferentiableWithinAt
      (((proj'_val' t).symm ‚ñ∏ hf).comp _
        ((smoothOn_proj'.mdifferentiableOn (val' t) (by simp [val',t.2.1,t.2.2])).mono
          (range_val' ‚ñ∏ s.image_subset_range _))
        (by simp [‚ÜêSet.preimage_comp,proj'_comp_val',subset_refl]))
      Set.subset_preimage_univ hst',
    mfderivWithin_univ,mfderiv_val',Function.comp_apply,proj'_val',
    mfderivWithin_comp _ ((proj'_val' t).symm ‚ñ∏ hf)
      ((smoothOn_proj'.mdifferentiableOn (val' t) (by simp [val',t.2.1,t.2.2])).mono
        (range_val' ‚ñ∏ s.image_subset_range _))
      (by simp [‚ÜêSet.preimage_comp,proj'_comp_val',subset_refl]) hst',
    mfderivWithin_subset (range_val' ‚ñ∏ s.image_subset_range val') hst'
      (smoothOn_proj'.mdifferentiableOn _ (by simp [val',t.2.1,t.2.2])),
    mfderivWithin_proj',proj'_val']
  ext
  by_cases ht : t.val < 1
  all_goals by_cases hft : (f t).val < 1
  simp_rw [ht,hft,and_self,true_or,ite_true,ContinuousLinearMap.coe_comp',Function.comp_apply]
  rfl
  simp_rw [ht,hft,ht.ne,false_and,ite_true,ite_false,ContinuousLinearMap.neg_comp,neg_neg,
    ContinuousLinearMap.coe_comp',Function.comp_apply]
  rfl
  simp_rw [ht,hft,hft.ne,and_false,ite_true,ite_false,ContinuousLinearMap.comp_neg,neg_neg,
    ContinuousLinearMap.coe_comp',Function.comp_apply]
  rfl
  simp_rw [ht,hft,eq_of_le_of_not_lt t.2.2 ht,eq_of_le_of_not_lt (f t).2.2 hft,or_true,ite_true,
    ite_false,ContinuousLinearMap.comp_neg,ContinuousLinearMap.neg_comp,neg_neg,
    ContinuousLinearMap.coe_comp',Function.comp_apply]
  rfl

lemma unitInterval.smooth_symm : Smooth (ùì°‚àÇ 1) (ùì°‚àÇ 1) unitInterval.symm := fun t => by
  apply smoothWithinAt_iff_contDiffWithinAt.mpr
  have h : ‚àÄ x ‚àà val' '' Set.univ, (val' ‚àò unitInterval.symm ‚àò proj') x =
      (Function.const ‚Ñù¬π (EuclideanSpace.single 0 1 : ‚Ñù¬π) - @id ‚Ñù¬π) x := fun x hx => by
    have hx' := (range_val' ‚ñ∏ (Set.image_univ ‚ñ∏ hx)).out
    simp [val',proj',Set.coe_projIcc,hx'.1,hx'.2,EuclideanSpace.single_sub,EuclideanSpace.single_eq_self]
  refine' ContDiffWithinAt.congr' _ h (Set.image_univ ‚ñ∏ (Set.mem_range_self t))
  refine' contDiffWithinAt_const.sub contDiffWithinAt_id

